/*  FOOTER  */

.Footer { width: 760px; text-align: left; color: #cccccc; background-image: url(http://l.yimg.com/www.flickr.com/images/dotted.gif); background-repeat: repeat-x; background-position: left top; margin: 10px auto 0 auto; padding: 10px; }
